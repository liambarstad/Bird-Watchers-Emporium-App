{
  "name": "bird-watchers-emporium-cloudformation",
  "version": "1.0.0",
  "description": "CloudFormation stacks for Bird Watchers' Emporium",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "deploy": "npm run build && node dist/scripts/deploy.js",
    "deploy:frontend": "npm run build && node dist/scripts/s3-deploy.js",
    "deploy:infrastructure": "cdk deploy --require-approval never",
    "deploy:all": "npm run deploy:infrastructure && npm run deploy:frontend",
    "clean": "rimraf dist",
    "synth": "cdk synth"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.450.0",
    "@aws-sdk/client-s3": "^3.450.0",
    "@aws-sdk/client-cloudfront": "^3.450.0",
    "@aws-sdk/s3-request-presigner": "^3.450.0",
    "aws-cdk-lib": "^2.100.0",
    "constructs": "^10.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.1.6",
    "rimraf": "^5.0.1",
    "aws-cdk": "^2.100.0",
    "mime-types": "^2.1.35"
  },
  "keywords": ["aws", "cloudformation", "typescript", "infrastructure"],
  "author": "",
  "license": "MIT"
}
